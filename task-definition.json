{
  "family": "picus-task",
  "networkMode": "hostPort",
  "containerDefinitions": [
    {
      "name": "picus-container",
      "image": "888577048636.dkr.ecr.us-east-1.amazonaws.com/picus-app",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::888577048636:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::888577048636:role/ecsTaskExecutionRole"
}